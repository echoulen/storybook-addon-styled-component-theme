{
  "name": "@mmit-erp/storybook-addon-jss-component-theme",
  "description": "mmit-erp storybook-addon-jss-component-theme",
  "homepage": "https://mmit-erp.github.io/storybook-addon-jss-component-theme/",
  "version": "2.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p .",
    "rollupBuild": "yarn build:clean && yarn build:lib",
    "build:watch": "npm run clean && tsc -p . -w",
    "pretest": "tslint -p .",
    "test": "rimraf dist && jest",
    "build:clean": "rimraf dist",
    "prepare": "npm run build",
    "coverage": "jest --coverage",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx}' --ignore-path ./.prettierignore --write",
    "types": "yarn tsc --noEmit",
    "finish": "yarn lint && yarn types && yarn test",
    "build:lib": "rollup -c",
    "semantic-release": "npx semantic-release"
  },
  "author": "plastininikolay",
  "license": "MIT",
  "repository": "mmit-erp/storybook-addon-jss-component-theme",
  "dependencies": {
    "immutable": "^4.0.0",
    "recompose": "^ 0.30.0",
    "rollup-plugin-copy": "^3.4.0",
    "webpack": "^5.72.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addons": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.0",
    "@types/recompose": "^0.30.10",
    "@types/sinon": "^10.0.11",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "jest": "^28.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-jss": "^10.9.1-alpha.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-url": "^3.0.1",
    "semantic-release": "^19.0.2",
    "sinon": "^13.0.2",
    "ts-jest": "^27.1.4",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@storybook/addons": ">=6.1.0",
    "@storybook/react": ">=5.2.0",
    "react-jss": ">=10.9.1-alpha.0"
  },
  "keywords": [
    "storybook",
    "jss",
    "addon",
    "theme",
    "react",
    "typescript",
    "storybook-addon",
    "style"
  ],
  "storybook": {
    "icon": "https://cssinjs.org/images/favicon.ico",
    "displayName": "jss theme"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"]
}
