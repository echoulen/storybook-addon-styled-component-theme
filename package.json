{
  "name": "@mmit-erp/storybook-addon-jss-component-theme",
  "description": "mmit-erp storybook-addon-jss-component-theme",
  "homepage": "https://mmit-erp.github.io/storybook-addon-jss-component-theme/",
  "version": "2.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p .",
    "rollupBuild": "yarn build:clean && yarn build:lib",
    "build:watch": "npm run clean && tsc -p . -w",
    "pretest": "tslint -p .",
    "test": "rimraf dist && jest",
    "build:clean": "rimraf dist",
    "prepare": "npm run build",
    "coverage": "jest --coverage",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx}' --ignore-path ./.prettierignore --write",
    "types": "yarn tsc --noEmit",
    "finish": "yarn lint && yarn types && yarn test",
    "build:lib": "rollup -c",
    "semantic-release": "npx semantic-release"
  },
  "author": "plastininikolay",
  "license": "MIT",
  "repository": "mmit-erp/storybook-addon-jss-component-theme",
  "dependencies": {
    "immutable": "^3.8.2",
    "recompose": "^0.27.1",
    "rollup-plugin-copy": "^3.4.0"
  },
  "devDependencies": {
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^10.3.3",
    "@types/react": "^16.14.5",
    "@types/recompose": "^0.26.1",
    "@types/sinon": "^5.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.2.0",
    "react-jss": "^10.9.1-alpha.2",
    "ts-jest": "^26.5.3",
    "tslint": "^5.11.0",
    "typescript": "^3.9.2",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-url": "^3.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "semantic-release": "^17.2.3"
  },
  "peerDependencies": {
    "@storybook/addons": ">=6.1.0",
    "@storybook/react": ">=5.2.0",
    "react-jss": ">=10.9.1-alpha.0"
  },
  "keywords": [
    "storybook",
    "jss",
    "addon",
    "theme",
    "react",
    "typescript",
    "storybook-addon",
    "style"
  ],
  "storybook": {
    "icon": "https://cssinjs.org/images/favicon.ico",
    "displayName": "jss theme"
  }
}
